    "rimraf": "^2.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  }
